{
  "name": "dreamlytics",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "clean": "find . -name '._*' -type f -delete && echo 'Cleaned up all ._* files'",
    "cap:init": "npx cap init",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "build:mobile": "nuxt generate && npx cap sync",
    "ios": "npm run build:mobile && npx cap open ios",
    "android": "npm run build:mobile && npx cap open android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.0",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@capacitor/toast": "^7.0.2",
    "@ionic/vue": "^8.7.9",
    "@ionic/vue-router": "^8.7.9",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@openrouter/sdk": "^0.1.11",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "ionicons": "^8.0.13",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.0",
    "nuxt": "^3.13.0",
    "openai": "^6.9.0",
    "vue": "^3.4.0",
    "vue-router": "^4.4.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "sass": "^1.77.0",
    "typescript": "^5.5.0"
  }
